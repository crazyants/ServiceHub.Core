@{
    Layout = "_LayoutEmpty";
}
@model LoginViewModel
<link rel="import" href="/elements/page-login/page-login.html" />

<page-login redirect-url="@Model.ReturnUrl"
            enable-local="@Model.EnableLocalLogin"
            allow-remember="@Model.AllowRememberLogin"
            external-providers="@JsonConvert.SerializeObject(Model.ExternalProviders)">
</page-login>